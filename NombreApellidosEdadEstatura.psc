Proceso NombreApellidosEdadEstatura
	definir nombre Como Caracter;
	definir apellidos Como Caracter;
	definir edad Como Entero;
	definir estatura Como Real;
	
	escribir "¿Cuál es tu nombre?";
	leer nombre;
	escribir "¿Cuáles son tus apellidos?";
	leer apellidos;
	escribir "¿Qué edad tienes?";
	leer edad;
	escribir "¿Cuánto mides? (Recuerde escribir su estatura con punto en vez de coma)";
	leer estatura;
	
FinProceso
