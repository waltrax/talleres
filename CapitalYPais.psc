Proceso CapitalYPais
	definir capital Como Caracter;
	definir pais Como Caracter;
	
	escribir "Escriba el nombre de alguna ciudad capital:";
	leer capital;
	escribir "Escriba el país del cuál es capital la ciudad anteriormente escrita:";
	leer pais;
	
	Limpiar Pantalla;
	
	escribir "La ciudad ",capital,", es la capital del país ",pais,".";
	
FinProceso
